{
  "name": "sharenest-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Collaborative text editor backend with MongoDB persistence",
  "main": "./dist/server.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --watch ./src -e ts --exec \"npm run build && npm run start\"",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/node": "^22.15.24",
    "@types/ws": "^8.18.1",
    "nodemon": "^3.1.10",
    "rimraf": "^5.0.10",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "mongodb": "^6.16.0",
    "ws": "^8.18.2",
    "y-websocket": "^3.0.0",
    "yjs": "^13.6.27"
  },
  "keywords": [
    "collaborative",
    "text-editor",
    "yjs",
    "mongodb",
    "websocket"
  ],
  "author": "Sharenest Team",
  "license": "MIT"
}
